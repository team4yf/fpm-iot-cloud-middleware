    "lodash": "^4.17.11",
    "yf-fpm-server": "^2.6.1"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "fpmc-jssdk": "^1.1.2",
    "mocha": "^6.0.2",
    "nodemon": "^2.0.2"
  }
}
